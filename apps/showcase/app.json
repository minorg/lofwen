{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "io.github.minorg.lofwen.showcase"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ea27b25f-6c98-4f59-bbe8-a572f4dd725a"
      },
      "router": {
        "origin": false
      }
    },
    "icon": "./assets/images/icon.png",
    "ios": {
      "supportsTablet": true
    },
    "name": "lofwen-showcase",
    "newArchEnabled": true,
    "orientation": "portrait",
    "platforms": ["android", "ios"],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store"
    ],
    "scheme": "lofwen-showcase",
    "slug": "lofwen-showcase",
    "userInterfaceStyle": "automatic",
    "version": "1.0.0"
  }
}
