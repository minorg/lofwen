{
  "dependencies": {
    "@clerk/clerk-expo": "~2.14.12",
    "@expo/vector-icons": "~14.1.0",
    "@lofwen/event-log": "*",
    "@lofwen/models": "*",
    "@react-navigation/native": "^7.0.14",
    "@rn-primitives/label": "~1.1.0",
    "@rn-primitives/radio-group": "~1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "class-variance-authority": "~0.7.1",
    "clsx": "~2.1.1",
    "envalid": "~8.0.0",
    "expo": "^53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-navigation-bar": "~4.2.7",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "nativewind": "~4.1.23",
    "partysocket": "~1.1.3",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-logs": "~5.3.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "5.4.0",
    "tailwind-merge": "~3.1.0",
    "tailwindcss": "~3.4.17",
    "tailwindcss-animate": "~1.0.7",
    "ts-invariant": "~0.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "~7.25.2",
    "@testing-library/react-native": "~13.2.0",
    "@types/jest": "~29.5.12",
    "@types/node": "^22.13.9",
    "@types/react": "19.0.12",
    "expo-doctor": "~1.12.8",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "react-test-renderer": "19.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/data.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@rn-primitives/.*)"
    ]
  },
  "license": "Apache-2.0",
  "main": "expo-router/entry",
  "name": "@lofwen/showcase-app",
  "private": true,
  "scripts": {
    "build:android:development:local": "eas build --local --output android-development-build.apk --platform android --profile development",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:preview:local": "eas build --local --output android-preview-build.apk --platform android --profile preview",
    "build:ios:development:local": "eas build --local --output ios-development-build.tar.gz --platform ios --profile development",
    "build:ios:preview": "eas build --platform ios --profile preview",
    "build:ios:preview:local": "eas build --local --output ios-preview-build.tar.gz --platform ios --profile preview",
    "clean": "rimraf .expo node_modules",
    "css2json": "../../tools/css2json/css2json.js < global.css >global.css.json",
    "depcheck": "depcheck .",
    "expo-doctor": "expo-doctor",
    "dev": "tsc --noEmit -w",
    "postinstall": "cd ../.. && npm run build",
    "start": "expo start",
    "start:android": "expo start -c --android",
    "start:ios": "expo start -c --ios",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit"
  },
  "version": "0.0.0"
}
