{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "2.0.0",

  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "ESNext"
  },
  "extends": ["./base.json"]
}
