{
  "extends": ["expo/tsconfig.base", "@tsconfig/strictest"],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": false,
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
